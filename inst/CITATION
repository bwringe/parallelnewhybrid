url = "https://github.com/bwringe/parallelnewhybrid",
doi = "complete",
textVersion = paste("Brendan Wringe, Ryan Stanley, Nicholas Jeffery, Eric Anderson, Ian Bradbury, (2016). parallelnewhybrid: an R package for the parallelization of hybrid detection using NEWHYBRIDS. R package version 0.0.0.9002. https://github.com/bwringe/parallelnewhybrid. doi : to_complete")
)
